{
  "name": "eloranker",
  "version": "0.0.1",
  "author": "Hrishi Olickel <twitter-@hrishioa> (https://olickel.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/southbridgeai/eloranker.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "description": "Elo ranking package with intelligent comparison selection and rating updates",
  "files": [
    "dist",
    "package.json"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly --declaration --declarationDir dist && mv dist/index.d.ts dist/index.d.mts && cp dist/index.d.mts dist/index.d.cts",
    "test": "jest"
  },
  "type": "module",
  "types": "./dist/index.d.cts",
  "devDependencies": {
    "@swc/core": "^1.7.36",
    "@types/jest": "^29.5.13",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  }
}
